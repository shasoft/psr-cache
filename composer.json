{
  "name": "shasoft/psr-cache",
  "description": "PSR-16 cache adapters",
  "license": "MIT",
  "keywords": [
    "cache",
    "psr-16",
    "php array"
  ],
  "require": {
    "psr/cache": "^3",
    "shasoft/filesystem": "*"
  },
  "autoload": {
    "psr-4": {
      "Shasoft\\PsrCache\\": "classes"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "robiningelbrecht/phpunit-pretty-print": "^1.2"
  },
  "autoload-dev": {
    "psr-4": {
      "Shasoft\\PsrCache\\Tests\\": "tests/classes"
    }
  },
  "authors": [
    {
      "name": "Shabanov Valera",
      "email": "git-shasoft-psr-cache@shasoft.com"
    }
  ],
  "minimum-stability": "dev"
}